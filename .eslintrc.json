{
    "extends": "airbnb-base",
    "plugins": ["react"],
    "settings": {
    "react": {
      "version": "detect" // или укажите версию React, которую вы используете, например, "17.0"
    }
  },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ]
   }
}